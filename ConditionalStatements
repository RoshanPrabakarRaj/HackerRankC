#include <numeric>
#include <sstream>
#include <iostream>
#include <algorithm>
using namespace std;


int main(){
    int N;
    cin >> N;
    if((N%2)!=0)
        {
        cout<<"Weird";
        }
    else if(N==2||N==4)
        {
        cout<<"Not Weird";
    }
    else if(N==6||N==8||N==10||N==12||N==14||N==16||N==18||N==20)
    {
        cout<<"Weird";
    }
    else if (N>20&&N<=100)
        {
        cout<<"Not Weird";
    }
    return 0;
    
}
